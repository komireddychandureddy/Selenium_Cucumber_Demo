<?xml version="1.0" encoding="UTF-8"?>
<extentreports>
	<configuration>
		<!-- report theme -->

		<!-- standard, dark -->

		<theme>standard</theme>
		<!-- document encoding -->

		<!-- defaults to UTF-8 -->

		<encoding>UTF-8</encoding>
		<!-- protocol for script and stylesheets -->

		<!-- defaults to https -->

		<protocol>http</protocol>
		<!-- title of the document -->
		<documentTitle>Extent</documentTitle>
		<!-- report name - displayed at top-nav -->

		<reportName>Cucumber BDD Report</reportName>
		<!-- location of charts in the test view -->

		<!-- top, bottom -->

		<testViewChartLocation>top</testViewChartLocation>
		<!-- custom javascript -->

		<!-- view report without internet -->
		<offlineMode>false</offlineMode>

		<!-- settings to change component location -->
		<testViewChartLocation>top</testViewChartLocation>
		<chartVisibleOnOpen>true</chartVisibleOnOpen>
		<enableTimeline>true</enableTimeline>

		<!-- settings to enable/disable views -->
		<enableCategoryView>true</enableCategoryView>
		<enableAuthorView>true</enableAuthorView>
		<enableExceptionView>true</enableExceptionView>
		<enableTestRunnerLogsView>true</enableTestRunnerLogsView>
		<scripts>

<![CDATA[
$(document).ready(function() {
});
]]>
		</scripts>
		<!-- custom styles -->
		<styles>
<![CDATA[
]]>
		</styles>
	</configuration>
</extentreports>